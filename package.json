    "scripts": {
        "start": "node ./bin/www",
        "stop": "taskkill /f /im node.exe",
        "build": "del-cli public/js/app  && SET NODE_ENV=development&& webpack --mode development --config webpack.config.dev.js --progress --profile --watch",
        "build:prod": "del-cli public/js/app  && SET NODE_ENV=production&& webpack --mode production --config webpack.config.prod.js --progress --profile",
        "doc-docco": "docco ./**/*.js ./docs/details/",
        "test": "run-s build:prod test-protractor test-jasmine",
        "test-protractor": "protractor protractor.conf.js",
        "test-jasmine": "jasmine-node --matchall test/server",
        "test-karma": "tsc --p src/tsconfig.spec.json && karma start --reporters kjhtml",
        "postinstall": "npm install --global del-cli && del-cli public/js/app  && webpack --mode production --config webpack.config.prod.js --progress --profile"
    },
